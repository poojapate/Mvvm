<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.kotlinproject">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/logo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.KotlinProject"
        android:usesCleartextTraffic="true">
        <activity
            android:name=".PracticeActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.BottomActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.VideoPayerActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.ProfileActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.SplashActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Activity.LoginActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.SignupActivity"
            android:exported="true" />
        <activity
            android:name=".Activity.NewSignUpActivity"
            android:exported="true" />
        <activity
            android:name=".Activity.UserContactActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.TabLayoutActivity"
            android:exported="true" />
        <activity
            android:name=".Activity.RecycleActivity"
            android:exported="true" />
        <activity
            android:name=".Activity.SecondActivity"
            android:exported="false" />
        <activity
            android:name=".Activity.MainActivity"
            android:exported="true" />
    </application>

</manifest>